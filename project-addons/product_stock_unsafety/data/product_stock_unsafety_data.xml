<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record forcecreate="True" id="ir_cron_joking_index" model="ir.cron">
            <field name="name">Compute joking index</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field ref="product.model_product_product" name="model_id"/>
	        <field name="state">code</field>
            <field name="code">model.calc_joking_index()</field>
        </record>

        <record forcecreate="True" id="ir_cron_joking_index_temporal" model="ir.cron">
            <field name="name">Compute joking index temporal</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field ref="product.model_product_product" name="model_id"/>
            <field name="state">code</field>
            <field name="code">model.calc_joking_index_temporal()</field>
        </record>

        <record id="joking_category_filter" model="ir.config_parameter">
            <field name="key">joking.category.filter</field>
            <field name="value"></field>
        </record>

        <record id="joking_brand_filter" model="ir.config_parameter">
            <field name="key">joking.brand.filter</field>
            <field name="value"></field>
        </record>

        <record id="joking_brand_excluded" model="ir.config_parameter">
            <field name="key">joking.brand.excluded</field>
            <field name="value"></field>
        </record>

    </data>
</odoo>
